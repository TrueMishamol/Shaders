window/size/viewport_height=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://addons/": "red",
"res://assets/": "pink",
"res://scenes/": "purple",
"res://shaders/": "orange",
"res://visual/": "orange"
}
